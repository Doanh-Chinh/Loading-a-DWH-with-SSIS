                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select soh.SalesOrderNumber,
	ROW_NUMBER() over(partition by sod.SalesOrderID order by sod.SalesOrderDetailID) as SalesOrderLineNumber,
		soh.RevisionNumber,
		sod.ProductID as ProductAK,
		soh.CustomerID as CustomerAK,
		soh.SalesPersonID as EmployeeAK,
